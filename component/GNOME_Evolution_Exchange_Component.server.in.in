<oaf_info>
<!-- (control) -->
<oaf_server iid="OAFIID:GNOME_Evolution_Exchange_Component_Factory:@BASE_VERSION@"
	    type="shlib"
	    location="@COMPONENTDIR@/libevolution_exchange.so">
    <oaf_attribute name="repo_ids" type="stringv">
      <item value="IDL:GNOME/ObjectFactory:1.0"/>
    </oaf_attribute>

    <oaf_attribute name="name" type="string"
		   _value="Evolution Exchange Component"/>
</oaf_server>
<!-- (control) -->
<oaf_server iid="OAFIID:GNOME_Evolution_Exchange_Control:@BASE_VERSION@"
	    type="factory"
	    location="OAFIID:GNOME_Evolution_Exchange_Component_Factory:@BASE_VERSION@">
	
    <oaf_attribute name="repo_ids" type="stringv">
	<item value="IDL:GNOME/Evolution/Component:@BASE_VERSION@"/>
    </oaf_attribute>

    <oaf_attribute name="name" type="string" _value="Evolution Exchange Component"/>
    <oaf_attribute name="evolution:button_icon" type="string" value="@IMAGESDIR@/connector-mini.png"/>
    <oaf_attribute name="evolution:button_label" type="string" value="Exchange"/>
    <oaf_attribute name="evolution:button_sort_order" type="string" value="0"/>
    <oaf_attribute name="evolution:component_icon" type="string" value="connector.png"/>
    <oaf_attribute name="evolution:component_display_order" type="number" value="1"/>
</oaf_server>
</oaf_info>
